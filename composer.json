{
    "name": "doctrine-dbal-util/url-multitaxonomy-fosub-demo",
    "type": "project",
    "description": "Demo URL Taxonomy Management with FOS User",
    "license": "ISC",
    "authors": [
        {"name": "Jean-Bernard Addor"}
    ],
    "autoload": {
        "psr-4": {
        },
        "classmap": [
            "src/"
        ],
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3",
        "doctrine-dbal-util/cli-bundle": "0.1.x-dev@dev",
        "doctrine-dbal-util/connection": "0.1.x-dev@dev",
        "doctrine-dbal-util/connection-pagerfanta": "0.1.x-dev@dev",
        "doctrine-dbal-util/connection-ramsey-uuid": "0.1.x-dev@dev",
        "doctrine-dbal-util/dbal-contrib": "^2.5.12-dev@dev",
        "doctrine-dbal-util/url-multitaxonomy-pager-controller": "^0.1.0@dev",
        "doctrine-dbal-util/url-multitaxonomy-pagerfanta-twig": "^0.1.0@dev",
        "doctrine-dbal-util/url-multitaxonomy-schema": "^0.1.0@dev",
        "doctrine/doctrine-bundle": "^1.6",
        "fos-user-util/doctrine-dbal-util-crud-bundle": "^0.1.0@dev",
        "fos-user-util/user-doctrine-dbal-schema": "^0.1.0@dev",
        "fos-user-util/user-util": "^2.0@dev",
        "friendsofsymfony/user-bundle": "^2.0",
        "incenteev/composer-parameter-handler": "^2.1",
        "monolog/monolog": "^1.22",
        "php-di/invoker": "^2.0",
        "php-taxonomy/multitaxonomy-doctrine-dbal-util-pagerfanta-twig-controller": "^0.1.0@dev",
        "php-taxonomy/multitaxonomy-pagerfanta-twig-templates": "^0.1.0@dev",
        "php-taxonomy/multitaxonomy-doctrine-dbal-schema": "^0.1.0@dev",
        "ramsey/uuid": "^3.6",
        "ramsey/uuid-doctrine": "^1.3",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "sensiolabs/security-checker": "^4.0",
        "symfony-util/controller-using-templating-http-foundation-http-kernel": "^0.1@dev",
        "symfony/asset": "^2.3 || ^3.4",
        "symfony/form": "^2.3 || ^3.4",
        "symfony/lts": "^2.3 || ^3.4",
        "symfony/monolog-bridge": "^2.3 || ^3.4",
        "symfony/process": "^2.3 || ^3.4",
        "symfony/symfony": "^2.3 || ^3.4",
        "symfony/templating": "^2.3 || ^3.4",
        "symfony/translation": "^2.3 || ^3.4",
        "symfony/twig-bundle": "^2.3 || ^3.4",
        "symfony/validator": "^2.3 || ^3.4",
        "symfony/var-dumper": "^2.3 || ^3.4",
        "symfony/web-profiler-bundle": "^2.3 || ^3.4",
        "symfony/yaml": "^2.3 || ^3.4",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle"            : "^3.0",
        "friendsofphp/php-cs-fixer"            : "^2.0",
        "sensio/generator-bundle"              : "^3.1",
        "symfony/phpunit-bridge"               : "^3.3"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        },
        "branch-alias": null
    }
}
